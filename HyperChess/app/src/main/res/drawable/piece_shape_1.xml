<?xml version="1.0" encoding="utf-8"?>
<inset
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:insetBottom="5px"
    android:insetLeft="5px"
    android:insetRight="5px"
    android:insetTop="5px">
    <shape
        android:shape="oval" >
        <size
            android:width="16dp"
            android:height="16dp" />
        <solid
            android:color="#0000FF" />
        <stroke
            android:width="0dp" />
    </shape>
</inset>